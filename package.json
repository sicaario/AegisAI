{
  "name": "aegisai",
  "version": "1.0.0",
  "description": "The Black Box Flight Recorder for LLM Applications",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "npm run dev"
  },
  "keywords": [
    "llm",
    "observability",
    "datadog",
    "gemini",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
